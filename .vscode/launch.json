{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Flask",
            "type": "python",
            "pythonPath": "${workspaceRoot}/venv/bin/python3.12",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "application/app.py",
                "FLASK_ENV": "development"
            },
            "args": [
                "run",
                "--host=0.0.0.0",
                "--port=7091"
            ],
            "jinja": true
        }
    ]
  }
  